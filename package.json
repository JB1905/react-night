{
  "name": "react-night",
  "version": "1.0.0-alpha.0",
  "description": "night.js implementation for React",
  "author": "Jakub Biesiada",
  "license": "MIT",
  "main": "lib/react-night.min.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "release": "rimraf lib/* && npm run prettier && npm run lint && npm run build:dev && npm run build",
    "build": "webpack --env build",
    "build:dev": "webpack --env dev",
    "watch": "npm run build -- --watch",
    "watch:dev": "npm run build:dev -- --watch",
    "lint": "eslint 'src/**/*.tsx' --fix",
    "prettier": "prettier --write 'src/**/*.tsx'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JB1905/react-night.git"
  },
  "keywords": ["React", "hooks", "dark", "night", "switch", "sun"],
  "bugs": {
    "url": "https://github.com/JB1905/react-night/issues"
  },
  "homepage": "https://github.com/JB1905/react-night#readme",
  "dependencies": {
    "night.js": "^1.2.0"
  },
  "devDependencies": {
    "@types/react": "^16.9.11",
    "@typescript-eslint/parser": "^2.7.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.2.0",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.3",
    "prettier": "^1.19.1",
    "react": "^16.9.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.2",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{tsx}": ["npm run prettier", "npm run lint", "git add"]
  }
}

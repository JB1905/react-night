{
  "name": "react-night",
  "version": "1.0.0-alpha.0",
  "description": "night.js implementation for React",
  "author": "Jakub Biesiada",
  "license": "MIT",
  "main": "./lib/react-night.min.js",
  "scripts": {
    "release": "npm run prettier && npm run lint && npm run build:dev && npm run build",
    "build": "webpack --env build",
    "build:dev": "webpack --env dev",
    "watch": "npm run build -- --watch",
    "watch:dev": "npm run build:dev -- --watch",
    "lint": "eslint 'src/**/*.tsx' --fix",
    "prettier": "prettier --write 'src/**/*.tsx'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JB1905/react-night.git"
  },
  "keywords": [
    "React",
    "hooks",
    "dark",
    "night",
    "switch",
    "sun"
  ],
  "bugs": {
    "url": "https://github.com/JB1905/react-night/issues"
  },
  "homepage": "https://github.com/JB1905/react-night#readme",
  "dependencies": {
    "night.js": "^1.2.0"
  },
  "devDependencies": {
    "@types/react": "^16.8.23",
    "@typescript-eslint/parser": "^1.12.0",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-react-hooks": "^1.6.1",
    "husky": "^3.0.0",
    "lint-staged": "^9.2.0",
    "prettier": "^1.18.2",
    "ts-loader": "^6.0.4",
    "typescript": "^3.5.3",
    "webpack": "^4.35.3",
    "webpack-cli": "^3.3.5"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{tsx}": [
      "npm run prettier",
      "npm run lint",
      "git add"
    ]
  }
}
